<div id="StyleLoadingDetail" ng-controller="app.views.styleloadings.detail as vm">


    <div class="hpanel">
        <div class="panel-heading">
            <div class="panel-tools">
                <a class="showhide" ng-click="isCollapsed = !isCollapsed"><i class="fa fa-chevron-up"></i></a>
                <a class="closebox"><i class="fa fa-times"></i></a>
            </div>
            Style Loading Details
        </div>
        <div uib-collapse="isCollapsed" class="panel-body">

            <div class="event-actions">
                <a class="btn btn-sm btn-primary" ng-href="#/styleloadings" angular-ripple><i class="fa fa-arrow-circle-left"></i> back to Style Loading</a>

            </div>



            <div class="panel-heading margin">
                <h3 class="block">{{vm.styleloading.styleNo}} Details details</h3>
            </div>

            <div class="panel-body">
                <div class="row margin">
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Style No</label>
                        <div class="controls">

                            <div> {{vm.styleloading.styleNo}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Line No</label>
                        <div class="controls">

                            <div> {{vm.styleloading.lineNo}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>
                </div>



                <div class="row margin">
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">From Date</label>
                        <div class="controls">

                            <div> {{vm.styleloading.fromDate | date:'yyyy/MM/dd'}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div class="col-md-6 col-sm-12 margin">
                        <label class="control-label">To Date</label>
                        <div class="controls">

                            <div>{{vm.styleloading.toDate | date:'yyyy/MM/dd'}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                </div>

                <div class="row margin">

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Remark</label>
                        <div class="controls">

                            <div> {{vm.styleloading.remark }} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                </div>

            </div>


          

        </div>
    </div>



