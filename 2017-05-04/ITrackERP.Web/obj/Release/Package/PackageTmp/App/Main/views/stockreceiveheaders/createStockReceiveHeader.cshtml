
<div id="stockreceiveheader">
    <route-loading-indicator></route-loading-indicator>
    <form name="stockreceiveheaderCreateForm" role="form" novalidate class="form-validation">




        <div class=" margin">
            <h3 class="block">{{vm.stockreceiveheader.receiveNoteNo}} Details </h3>
        </div>

        <div class="panel-body">
            <div class="row margin">
               
                <div class="col-md-6 col-sm-12 margin">
                    <label class="control-label">Date </label>
                    <p class="input-group">
                        <input type="text" class="form-control" uib-datepicker-popup ng-model="rdate" is-open="dpOpenStatus.rdate" datepicker-options="dateOptions" close-text="Close" alt-input-formats="altInputFormats" />

                        <span class="input-group-btn">
                            <button type="button"
                                    class="btn btn-default"
                                    ng-click="setDpOpenStatus('rdate', $event)">
                                <i class="glyphicon glyphicon-calendar"></i>
                            </button>
                        </span>
                    </p>
                </div>
               
                <div class="col-md-6 col-sm-12">
                    <label class="control-label">Remark </label>
                    <div class="controls">
                        <input type="text" class="form-control" ng-model="vm.stockreceiveheader.remark" name="remark" ng-maxlength="100" ng-messages="stockreceiveheaderCreateForm.remark.$error" />

                        <div ng-messages="stockreceiveheaderCreateForm.remark.$error" ng-if="stockreceiveheaderCreateForm.remark.$touched">

                            <div ng-messages-include="/App/Main/views/messages.html"></div>
                        </div>
                    </div>
                </div>
            </div>
           
                <hr />

                <div class="event-actions">



                    <a class="btn btn-sm btn-green" ng-click="vm.save()"><i class="fa fa-arrow-circle-left"></i> Save </a>

                    <a class="btn btn-sm btn-blue" ng-click="vm.cancel()"><i class="fa fa-arrow-circle-left"></i> Cancel</a>

                </div>


            </div>

    </form>


</div>