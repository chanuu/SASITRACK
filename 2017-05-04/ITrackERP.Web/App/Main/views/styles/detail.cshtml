<div id="StyleDetail" ng-controller="app.views.styles.detail as vm">
    <route-loading-indicator></route-loading-indicator>
    <div class="hpanel">
        <div class="panel-heading">
            <div class="panel-tools">
                <a class="showhide" ng-click="isCollapsed = !isCollapsed"><i class="fa fa-chevron-up"></i></a>
                <a class="closebox"><i class="fa fa-times"></i></a>
            </div>
            Style Master
        </div>

        <div uib-collapse="isCollapsed" class="panel-body">
            <div class="event-actions">
                <a class="btn btn-primary btn-sm btn-blue" ng-click="vm.backToEventsPage()"><i class="fa fa-arrow-circle-left"></i> back to style</a>

            </div>
            <hr />


            
                <h3 class="block">{{vm.style.styleNo}} Details</h3>
          

            <div class="panel-body">
                <div class="row margin">
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Style No</label>
                        <div class="controls">
                            <div  class="" >{{vm.style.styleNo}}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Article No</label>
                        <div class="controls">
                            <div  class="">{{vm.style.articleNo}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>
                </div>



                <div class="row margin">
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Boc No</label>
                        <div class="controls">
                            <div  class="" >{{vm.style.bocNo}}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div class="col-md-6 col-sm-12 margin">
                        <label class="control-label">Season</label>
                        <div class="controls">
                            <div  class="" >{{vm.style.season}}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                </div>

                <div class="row margin">

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Item Type</label>
                        <div class="controls">
                            <div class="">{{vm.style.itemType}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div class="col-md-6 col-sm-12 margin">
                        <label class="control-label">Department</label>
                        <div class="controls">
                            <div  class="">{{vm.style.department}}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                </div>
                <div class="row margin">

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Order Type</label>
                        <div class="controls">
                            <div class="">{{vm.style.orderType}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div class="col-md-6 col-sm-12 margin">
                        <label class="control-label">Buyer Name</label>
                        <div class="controls">
                            <div class="">{{vm.style.buyerName}}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                </div>
                <div class="row margin">

                    <div class="col-md-6 col-sm-12 margin">
                        <label class="control-label">Remark</label>
                        <div class="controls">
                            <div> {{vm.style.remark}}</div>

                            <span class="help-inline"></span>

                        </div>
                    </div>
                </div>




            </div>
        </div>

        </div>
 </div>