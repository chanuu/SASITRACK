<div ng-controller="app.views.dashboard.cutting.production as vm">
    <div class="row">
        <div class="col-lg-12">

            <div class="hpanel">
                <div class="panel-heading">
                    <div class="panel-tools">
                        <a class="showhide" ng-click="isCollapsed1 = !isCollapsed1"><i class="fa fa-chevron-up"></i></a>
                        <a class="closebox"><i class="fa fa-times"></i></a>
                    </div>
                    Filter Options / Data Configurations
                </div>
                <div uib-collapse="isCollapsed1" class="panel-body">

                    <div class="row margin-adjust">
                        <div class="col-md-6 col-sm-12">
                            <label class="control-label">From Date</label>
                            <div class="controls">
                                <input placeholder="yyyy-mm-dd" type="text" class="form-control" ng-model="filter.fromDate" required />
                                <span class="help-inline"></span>

                            </div>
                        </div>

                        <div class="col-md-6 col-sm-12">
                            <label class="control-label">To Date</label>
                            <div class="controls">
                                <input placeholder="yyyy-mm-dd" type="text" class="form-control" ng-model="filter.toDate" required />
                                <span class="help-inline"></span>

                            </div>
                        </div>


                        <hr />

                        <div class="event-actions">



                            <a class="btn btn-primary btn-green" ng-click="vm.process()"><i class="fa fa-arrow-circle-left"></i> Process </a>

                            <a class="btn btn-primary btn-blue" ng-click="vm.cancel()"><i class="fa fa-arrow-circle-left"></i> Cancel</a>

                        </div>

                    </div>


                </div>
            </div>
        </div>



    </div>


    <div class="row" ng-show="isProcessed==true">
        <div class="col-lg-12">
            <div class="hpanel">
                <div class="panel-heading">
                    <div class="panel-tools">
                        <a class="showhide" ng-click="isCollapsed2 = !isCollapsed2"><i class="fa fa-chevron-up"></i></a>
                        <a class="closebox"><i class="fa fa-times"></i></a>
                    </div>
                    Individual Effience Of V-2 2016-11-25
                </div>
                <div uib-collapse="isCollapsed2" class="panel-body">
                    <div class="col-lg-12">
                        <div class="ibox float-e-margins">
                            <div class="ibox-title">
                                <h5>
                                  
                                    <small>{{date | date:'yyyy-MM-dd'}}</small>
                                </h5>
                                <div ibox-tools></div>
                            </div>
                            <div class="ibox-content">
                                <div>
                                    <am-chart class="chart-m" id="myFirstChart" options="amChartOptions" height="700px" width="100%"></am-chart>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>




    </div>


</div>