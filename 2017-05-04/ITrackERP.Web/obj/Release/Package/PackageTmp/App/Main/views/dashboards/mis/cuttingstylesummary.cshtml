<div ng-controller="app.views.dashboard.mis.cuttingstylesummary as vm">


    <div class="row">
        <div class="col-lg-3 col-s-6">
            <a ng-href="">
                <div class="hpanel">

                    <div class="panel-body text-center h-200">
                        <i class="pe-7s-scissors fa-4x"></i>
                        <h3 class="m-xs">{{totaltableCut}}</h3>

                        <h3 class="font-extra-bold no-margins text-success">
                            Total Cut Qty
                        </h3>
                        <small>Completed Pcs</small>
                    </div>

                </div>
            </a>
        </div>
        <div class="col-lg-3 col-s-6">
            <a ng-href="#/dashboards/cutting/tablecut">
                <div class="hpanel">

                    <div class="panel-body text-center h-200">
                        <i class="pe-7s-users fa-4x"></i>
