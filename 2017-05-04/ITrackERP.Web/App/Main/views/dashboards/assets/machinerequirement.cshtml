<div ng-controller="app.views.dashboard.assets.machinerequirement as vm">

    <div class="row">
        <div class="col-lg-12">

            <div class="hpanel">
                <div class="panel-heading">
                    <div class="panel-tools">
                        <a class="showhide" ng-click="isCollapsed = !isCollapsed"><i class="fa fa-chevron-up"></i></a>
                        <a class="closebox"><i class="fa fa-times"></i></a>
                    </div>
                    Filter Options / Data Configurations
                </div>
                <div uib-collapse="isCollapsed" class="panel-body">

                    <div class="row margin-adjust">
                        <div class="col-md-12 col-sm-12">
                            <label class="control-label">From Date</label>
                            <p class="input-group">


                                <input type="text" class="form-control" uib-datepicker-popup ng-model="fdate" is-open="dpOpenStatus.fdate" datepicker-options="dateOptions" close-text="Close" alt-input-formats="altInputFormats" />

                                <span class="input-group-btn">
                                    <button type="button"
                                            class="btn btn-default"
                                            ng-click="setDpOpenStatus('fdate', $event)">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </button>
                                </span>
                            </p>
                        </div>

                     
                        <hr />

                        <div class="event-actions">



                            <a class="btn btn-primary btn-green" ng-click="vm.process()"><i class="fa fa-arrow-circle-left"></i> Process </a>

                            <a class="btn btn-primary btn-blue" ng-click="vm.cancel()"><i class="fa fa-arrow-circle-left"></i> Cancel</a>

                        </div>

                    </div>


                </div>
            </div>
        </div>



    </div>

   
    <div class="row">
        <div class="col-lg-12">

            <div class="hpanel">
                <div class="panel-heading">
                    <div class="panel-tools">
                        <a class="showhide" ng-click="isCollapsed1 = !isCollapsed1"><i class="fa fa-chevron-up"></i></a>
                        <a class="closebox"><i class="fa fa-times"></i></a>
                    </div>
                    Filter Options / Data Configurations
                </div>
                <div uib-collapse="isCollapsed1" class="panel-body">
                    
                    
              
                 <div id="container" class="n_container" >
                    <table class="table table-header-rotated">
                        <thead>
                            <tr>
                                
                                <th style="min-width:80px"></th>
                                <th style="min-width:80px"></th>
                                <th style="min-width:80px" class=""rotate min-width-th"" ng-repeat="machinerequirement in converteddata.machines"><div><span class="pull-right">{{machinerequirement.currentmachine}}</span></div></th>
                            </tr>
                            <tr ng-repeat="">
                                <td > </td>
                            </tr>
                           
                        </thead>
                        <tbody>
                            <tr ng-animate="'animate'" dir-paginate="machinerequirement in converteddata|orderBy:sortKey:reverse|filter:search|itemsPerPage:7">

                                <td>{{machinerequirement.lineNo}}</td>
                                <td>{{machinerequirement.styleNo}}</td>


                                <td ng-repeat="machine in machinerequirement.machines"><span class="pull-right">{{machine.total}}</span></td>
                            </tr>
                            <tr>
                                
                                <td colspan="2">Required Machines By Type</td>
                                <td  ng-repeat="machine in result"><span class="pull-right">Total : {{machine.Nos}}</span></td>
                            </tr>

                            <tr>

                                <td colspan="2">Available Machines By Type</td>
                                <td ng-repeat="machine in finalMachineResult"><span class="pull-right">: {{machine.nos}}</span></td>
                            </tr>

                            <tr>

                                <td colspan="2">Defference between </td>
                                <td ng-repeat="machine in finalMachineResult"><span class="pull-right">Defference : {{machine.deference}}</span></td>
                            </tr>


                           
                        </tbody>
                       
                    </table>
                 </div>

                </div>
            </div>
        </div>



    </div>
      
   



 


    </div>
