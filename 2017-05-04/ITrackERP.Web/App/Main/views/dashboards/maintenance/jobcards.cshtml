<div ng-controller="app.views.dashboard.maintenance.jobcards as vm">


    <div class="row">
        <div class="col-lg-12">

            <div class="hpanel">
                <div class="panel-heading">
                    <div class="panel-tools">
                        <a class="showhide" ng-click="isCollapsed1 = !isCollapsed1"><i class="fa fa-chevron-up"></i></a>
                        <a class="closebox"><i class="fa fa-times"></i></a>
                    </div>
                    Filter Options / Data Configurations
                </div>
                <div uib-collapse="isCollapsed1" class="panel-body">

                    <div class="row margin-adjust">
                        <div class="col-md-6 col-sm-12">
                            <label class="control-label">Asset No</label>
                            <div class="controls">
                                <input type="text" typeahead-on-select="setAssetNo($item)" ng-model="selectedAssetNo" uib-typeahead="asset as asset.assetNo for asset in vm.assets | filter:$viewValue | limitTo:8" class="form-control">
                                <span class="help-inline"></span>
                            </div>

                        </div>
                    </div>

                    <div class="row margin-adjust">
                        <div class="col-md-6 col-sm-12">
                            <label class="control-label">From Date</label>
                            <p class="input-group">

                                <input type="text" class="form-control" uib-datepicker-popup ng-model="fdate" is-open="dpOpenStatus.fdate" datepicker-options="dateOptions" close-text="Close" alt-input-formats="altInputFormats" />

                                <span class="input-group-btn">
                                    <button type="button"
                                            class="btn btn-default"
                                            ng-click="setDpOpenStatus('fdate', $event)">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </button>
                                </span>
                            </p>
                        </div>

                        <div class="col-md-6 col-sm-12">
                            <label class="control-label">To Date</label>
                            <p class="input-group">

                                <input type="text" class="form-control" uib-datepicker-popup ng-model="tdate" is-open="dpOpenStatus.tdate" datepicker-options="dateOptions" close-text="Close" alt-input-formats="altInputFormats" />

                                <span class="input-group-btn">
                                    <button type="button"
                                            class="btn btn-default"
                                            ng-click="setDpOpenStatus('tdate', $event)">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </button>
                                </span>
                            </p>
                        </div>




                        <div class="col-md-6">

                        </div>



                        <hr />

                        <div class="event-actions">



                            <a class="btn btn-primary btn-green" ng-click="vm.process()"><i class="fa fa-arrow-circle-left"></i> Process </a>

                            <a class="btn btn-primary btn-blue" ng-click="vm.cancel()"><i class="fa fa-arrow-circle-left"></i> Cancel</a>

                        </div>

                    </div>


                </div>
            </div>
        </div>



    </div>








</div>
