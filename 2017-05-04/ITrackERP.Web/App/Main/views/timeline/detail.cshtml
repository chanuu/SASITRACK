
<div id="machineRequirementDetail">

    <div class="hpanel">
        <div class="panel-heading">
            <div class="panel-tools">
                <a class="showhide" ng-click="isCollapsed = !isCollapsed"><i class="fa fa-chevron-up"></i></a>
                <a class="closebox"><i class="fa fa-times"></i></a>
            </div>
            Machine Requirement Details
        </div>
        <div uib-collapse="isCollapsed" class="panel-body">

            <div class="panel-heading margin">
             <!--  <h3 class="block">{{vm.machinerequirement.machineType}} Details details</h3>--> 
            </div>

            <div class="panel-body">
                <div class="row margin">
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Machine Type</label>
                        <div class="controls">

                            <div> {{vm.machinerequirement.machineType }} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Nos </label>
                        <div class="controls">

                            <div> {{vm.machinerequirement.nos}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>
                </div>

                <div class="row margin">
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Remark</label>
                        <div class="controls">

                            <div> {{vm.machinerequirement.remark}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                </div>

            </div>


            <div />



        </div>


        <div class="event-actions">

            <a class="btn btn-sm btn-blue" angular-ripple ng-click="vm.cancel()"><i class="fa fa-arrow-circle-left"></i> Close</a>

        </div>
    </div>
</div>
