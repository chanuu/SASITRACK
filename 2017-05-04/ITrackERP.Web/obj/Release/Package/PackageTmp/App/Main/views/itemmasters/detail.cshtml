                        <label class="control-label">Discount  </label>
                        <div class="controls">

                            <div>{{vm.itemmaster.discount  }}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                </div>

                <div class="row margin">

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Customer Return Order </label>
                        <div class="controls">

                            <div> {{vm.itemmaster.customerReturnOrder}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div class="col-md-6 col-sm-12 margin">
                        <label class="control-label">Serial Item </label>
                        <div class="controls">

                            <div>{{vm.itemmaster.serialItem }}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                </div>

                <div class="row margin">

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Image</label>
                        <div class="controls">

                            <div> {{vm.itemmaster.image}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>
                </div>
            </div>
          
        </div>
    </div>
</div>



