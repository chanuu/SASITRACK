<div id="FileDetail" ng-controller="app.views.files.detail as vm">
    <route-loading-indicator></route-loading-indicator>
    <div class="hpanel">
        <div class="panel-heading">
            <div class="panel-tools">
                <a class="showhide" ng-click="isCollapsed = !isCollapsed"><i class="fa fa-chevron-up"></i></a>
                <a class="closebox"><i class="fa fa-times"></i></a>
            </div>
            File
        </div>

        <div uib-collapse="isCollapsed" class="panel-body">
            
            <hr />

            <h3 class="block">{{vm.files.customField1 }} Details</h3>
            <div class="panel-body">
                <div class="row margin">
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Type </label>
                        <div class="controls">
                            <div class="">{{vm.files.type }}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>
         
                    <div ng-show="vm.customcaption1 != null" class="col-md-6 col-sm-12">
                        <label class="control-label">{{vm.customcaption1}}</label>
                        <div class="controls">
                            <div class="">{{vm.files.customField1 }}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>
                </div>

                <div class="row margin">
                    <div ng-show="vm.customcaption2 != null" class="col-md-6 col-sm-12">
                        <label class="control-label">{{vm.customcaption2}}</label>
                        <div class="controls">
                            <div class="">{{vm.files.customField2 }}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div ng-show="vm.customcaption3 != null" class="col-md-6 col-sm-12">
                        <label class="control-label">{{vm.customcaption3}}</label>
                        <div class="controls">
                            <div class="">{{vm.files.customField3 }}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>
                </div>

                <div class="row margin">
                    <div ng-show="vm.customcaption4 != null" class="col-md-6 col-sm-12">
                        <label class="control-label">{{vm.customcaption4}}</label>
                        <div class="controls">
                            <div class="">{{vm.files.customField4 }}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div ng-show="vm.customcaption5 != null" class="col-md-6 col-sm-12">
                        <label class="control-label">{{vm.customcaption5}}</label>
                        <div class="controls">
                            <div class="">{{vm.files.customField5 }}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>
                </div>

                <div class="row margin">
                    <div ng-show="vm.customcaption6 != null" class="col-md-6 col-sm-12">
                        <label class="control-label">{{vm.customcaption6}}</label>
                        <div class="controls">
                            <div class="">{{vm.files.customField6 }}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Category 1 </label>
                        <div class="controls">
                            <div class="">{{vm.files.category1}}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>
                </div>

                <div class="row margin">
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Category 2 </label>
                        <div class="controls">
                            <div class="">{{vm.files.category2 }}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Category 3 </label>
                        <div class="controls">
                            <div class="">{{vm.files.category3 }}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>
                </div>
                <div class="row margin">
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Category 4 </label>
                        <div class="controls">
                            <div class="">{{vm.files.category4 }}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Category 5 </label>
                        <div class="controls">
                            <div class="">{{vm.files.category5 }}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>
                </div>

                <div class="row margin">
                    <div ng-show="vm.files.path != 'N/A'" class="row margin">
                        <div class="col-md-6 col-sm-12">
                            <label class="control-label">File</label>
                            <div class="controls">
                                <iframe src="{{vm.files.path}}"> </iframe>
                            </div>
                        </div>
                    </div>

                   
                </div>

        </div>
    </div>
