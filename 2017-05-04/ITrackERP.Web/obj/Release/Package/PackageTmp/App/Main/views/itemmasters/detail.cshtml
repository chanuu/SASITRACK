                        <div class="controls">

                            <div>{{vm.itemmaster.minimumQty  }}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                </div>

                <div class="row margin">

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Batch Item </label>
                        <div class="controls">

                            <div> {{vm.itemmaster.batchItem  }} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div class="col-md-6 col-sm-12 margin">
                        <label class="control-label">Service Item </label>
