
<div id="createWorkOrder">

    <form name="workOrderCreateForm" role="form" novalidate class="form-validation">




        <div class=" margin">
            <h3 class="block">{{vm.workOrder.workOrderNo}} Details details</h3>
        </div>

        <div class="panel-body">
            <div class="row margin">
                <div class="col-md-6 col-sm-12">
                    <label class="control-label">Work Order No</label>
                    <div class="controls">
                        <input type="text" class="form-control" ng-model="vm.workOrder.workOrderNo" required />



                    </div>
                </div>
                <div class="col-md-6 col-sm-12">
                    <label class="control-label">Start Date</label>
                    <div class="controls">
                        <input type="text" class="form-control" ng-model="vm.workOrder.startDate" required />
                        <span class="help-inline"></span>

                    </div>
                </div>
            </div>



            <div class="row margin">
                <div class="col-md-6 col-sm-12">
                    <label class="control-label">End Date</label>
                    <div class="controls">
                        <input type="text" class="form-control" ng-model="vm.workOrder.endDate" />
                        <span class="help-inline"></span>

                    </div>
                </div>

                <div class="col-md-6 col-sm-12 margin">
                    <label class="control-label">Status</label>
                    <div class="controls">
                        <input type="text" class="form-control" ng-model="vm.workOrder.status" required />
                        <span class="help-inline"></span>

                    </div>
                </div>

            </div>

            <div class="row margin">

                <div class="col-md-6 col-sm-12">
                    <label class="control-label">Item Type</label>
                    <div class="controls">
                       

                        <select class="form-control" name="itemType" ng-model="vm.workOrder.itemType">
                            <option value="Shirt">Shirt</option>
                            <option value="Skirt">Skirt</option>
                            <option value="Bluose">Bluose</option>
                            <option value="Jacket">Jacket</option>
                        </select>


                        <span class="help-inline"></span>

                    </div>
                </div>

                <div class="col-md-6 col-sm-12 margin">
                    <label class="control-label">Priority</label>
                    <div class="controls">
                        <input type="text" class="form-control" ng-model="vm.workOrder.priority" required />
                        <span class="help-inline"></span>

                    </div>
                </div>

            </div>

            <div class="row margin">

                <div class="col-md-6 col-sm-12 margin">
                    <label class="control-label">Remark</label>
                    <div class="controls">
                        <input type="text" class="form-control" ng-model="vm.workOrder.remark" required />
                        <span class="help-inline"></span>

                    </div>
                </div>
                <div class="col-md-6 col-sm-12 margin">
                    <label class="control-label">Style No</label>
                    <div class="controls">
                        <input type="text" typeahead-on-select="setStyle($item)" ng-model="selectedStyle" uib-typeahead="style as style.styleNo for style in vm.styles | filter:$viewValue | limitTo:8" class="form-control">
                        <span class="help-inline"></span>
                    </div>
                   
                </div>


            </div>

     
            <hr />

            <div class="event-actions">



                <a class="btn btn-sm btn-green" angular-ripple ng-click="vm.save()"><i class="fa fa-arrow-circle-left"></i> Save </a>

                <a class="btn btn-sm btn-blue" angular-ripple ng-click="vm.cancel()"><i class="fa fa-arrow-circle-left"></i> Cancel</a>

            </div>


        </div>

    </form>


</div>