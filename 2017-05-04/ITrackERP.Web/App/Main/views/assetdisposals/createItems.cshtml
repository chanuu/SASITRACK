
<div id="createAssetDisposalHeader">

    <form name="assetDisposalHeaderItemCreateForm" role="form" novalidate class="form-validation">

        <div class=" margin">
            <h3 class="block">{{vm.assetdisposal.assetDisposalHeaderId }} Details details</h3>
        </div>

        <div class="panel-body">
            <div class="row margin">
               
                <div class="col-md-6 col-sm-12 margin">
                    <label class="control-label">Asset No</label>
                    <div class="controls">
                        <input type="text" typeahead-on-select="setAssetNo($item)" ng-model="selectedAssetNo" uib-typeahead="assetno as assetno.assetNo for assetno in vm.assets | filter:$viewValue | limitTo:8" class="form-control" required>
                        <span class="help-inline"></span>                      
                    </div>
                </div>



                <div class="col-md-6 col-sm-12">
                    <label class="control-label">Description</label>                  
                    <div class="controls">
                        <input type="text" class="form-control" ng-model="assetdisposaldetail.description" name="description" ng-maxlength="100" ng-messages="assetDisposalHeaderItemCreateForm.description.$error"/>

                        <div ng-messages="assetDisposalHeaderItemCreateForm.description.$error" ng-if="assetDisposalHeaderItemCreateForm.description.$touched">

                            <div ng-messages-include="/App/Main/views/messages.html"></div>
                        </div>
                    </div>
                </div>
            </div>


            <hr />

            <div class="event-actions">



                <a class="btn btn-sm btn-green" angular-ripple ng-click="vm.save()"><i class="fa fa-arrow-circle-left"></i> Save </a>

                <a class="btn btn-sm btn-blue" angular-ripple ng-click="vm.cancel()"><i class="fa fa-arrow-circle-left"></i> Cancel</a>

            </div>


        </div>

    </form>


</div>