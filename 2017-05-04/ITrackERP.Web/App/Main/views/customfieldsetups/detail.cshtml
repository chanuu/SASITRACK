<div id="CustomFieldSetupDetail" ng-controller="app.views.customfieldsetups.detail as vm">
    <route-loading-indicator></route-loading-indicator>
    <div class="hpanel">
        <div class="panel-heading">
            <div class="panel-tools">
                <a class="showhide" ng-click="isCollapsed = !isCollapsed"><i class="fa fa-chevron-up"></i></a>
                <a class="closebox"><i class="fa fa-times"></i></a>
            </div>
           Custom Field Setup
        </div>

        <div uib-collapse="isCollapsed" class="panel-body">
            <div class="event-actions">
                <a class="btn btn-primary btn-sm btn-blue" ng-click="vm.backToEventsPage()"><i class="fa fa-arrow-circle-left"></i> back to custome field setup</a>

            </div>
            <hr />



            <h3 class="block">{{vm.customfieldsetup.customFieldNo}} Details</h3>


            <div class="panel-body">
                <div class="row margin">
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Custom Field No </label>
                        <div class="controls">
                            <div class="">{{vm.customfieldsetup.customFieldNo }}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Item Type </label>
                        <div class="controls">
                            <div class="">{{vm.customfieldsetup.itemType }} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>
                </div>



                <div class="row margin">
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Custom Field 1 </label>
                        <div class="controls">
                            <div class="">{{vm.customfieldsetup.customeField1}}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Custom Field 2 </label>
                        <div class="controls">
                            <div class="">{{vm.customfieldsetup.customeField2}}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                </div>

                <div class="row margin">
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Custom Field 3 </label>
                        <div class="controls">
                            <div class="">{{vm.customfieldsetup.customeField3}}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Custom Field 4 </label>
                        <div class="controls">
                            <div class="">{{vm.customfieldsetup.customeField4}}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                </div>

                <div class="row margin">
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Custom Field 5 </label>
                        <div class="controls">
                            <div class="">{{vm.customfieldsetup.customeField5}}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Custom Field 6 </label>
                        <div class="controls">
                            <div class="">{{vm.customfieldsetup.customeField6}}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                </div>

                <div class="row margin">

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Code Length</label>
                        <div class="controls">
                            <div type="text" class="">{{vm.customfieldsetup.codeLength}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Item Code Generate</label>
                        <div class="controls">
                            <div class="">{{vm.customfieldsetup.itemCodeGenerate}}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                </div>





            </div>
        </div>

    </div>
</div>