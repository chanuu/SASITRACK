<div id="ItemMasterDetail" ng-controller="app.views.itemmasters.detail as vm">


    <div class="hpanel">
        <div class="panel-heading">
            <div class="panel-tools">
                <a class="showhide" ng-click="isCollapsed = !isCollapsed"><i class="fa fa-chevron-up"></i></a>
                <a class="closebox"><i class="fa fa-times"></i></a>
            </div>
            Item Masters
        </div>
        <div uib-collapse="isCollapsed" class="panel-body">

            <div class="event-actions">
                <a class="btn btn-sm btn-primary" ng-href="#/itemmasters" angular-ripple><i class="fa fa-arrow-circle-left"></i> back to Item Masters</a>

            </div>



            <div class="panel-heading margin">
                <h3 class="block">{{vm.itemmaster.itemType }} Details details</h3>
            </div>



            <div class="panel-body">
                <div class="row margin">
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Item Type</label>
                        <div class="controls">

                            <div> {{vm.itemmaster.itemType}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Item No</label>
                        <div class="controls">

                            <div> {{vm.itemmaster.itemNo}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>
                </div>



                <div class="row margin">
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">CustomField 1</label>
                        <div class="controls">

                            <div> {{vm.itemmaster.customField1}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">CustomField 2</label>
                        <div class="controls">

                            <div> {{vm.itemmaster.customField2}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                </div>

                <div class="row margin">
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">CustomField 3</label>
                        <div class="controls">

                            <div> {{vm.itemmaster.customField3}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">CustomField 4</label>
                        <div class="controls">

                            <div> {{vm.itemmaster.customField4}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                </div>

                <div class="row margin">
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">CustomField 5</label>
                        <div class="controls">

                            <div> {{vm.itemmaster.customField5}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">CustomField 6</label>
                        <div class="controls">

                            <div> {{vm.itemmaster.customField6}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                </div>

                <div class="row margin">
                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Supplier</label>
                        <div class="controls">

                            <div> {{vm.itemmaster.supplier}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Uom</label>
                        <div class="controls">

                            <div> {{vm.itemmaster.uom}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                </div>

                <div class="row margin">

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Status</label>
                        <div class="controls">

                            <div> {{vm.itemmaster.status}} </div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                    <div class="col-md-6 col-sm-12 margin">
                        <label class="control-label">Max Qty </label>
                        <div class="controls">

                            <div>{{vm.itemmaster.maxQty }}</div>
                            <span class="help-inline"></span>

                        </div>
                    </div>

                </div>

                <div class="row margin">

                    <div class="col-md-6 col-sm-12">
                        <label class="control-label">Re Order Qty </label>
                        <div class="controls">

